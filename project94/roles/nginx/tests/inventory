all:
  vars:
    ansible_ssh_private_key_file: ~/.ssh/id_ansible
    ansible_python_interpreter: /usr/bin/python3

  children:
    webservers:
      vars:
        ansible_user: yakmat
      hosts:
        web01:
          ansible_host: "{{ web01_ip }}"
        web02:
          ansible_host: "{{ web02_ip }}"
          ansible_user: test

    dbservers:
      hosts:
        db1:
          ansible_host: "{{ db1_ip }}"
          ansible_user: racdemo
          ansible_python_interpreter: /usr/bin/python3.6



